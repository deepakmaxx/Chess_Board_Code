#include <iostream>
using namespace std;

int main() {
    // Write C++ code here
    int x,y;
    cout<<"enter row";
    cin>>x;
    
    for (int i = 1; i <= x; i++) {
        for (int j = 1; j <= x; j++) {
         

            if ((i + j) % 2 == 0) {
                cout<<"White ";
            } else {
                cout<<"Black ";
            }   
         
        }
        cout<<endl;
    }
    return 0;
}
